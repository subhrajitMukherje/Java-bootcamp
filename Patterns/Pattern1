package pattern;

import java.util.Scanner;
public class pa1 {

	public static void main(String[] args) {
		
		Scanner sc=new Scanner(System.in);
		System.out.print("enter the number");
		int n=sc.nextInt();
		sc.close();
		printPattern(n);
	}
private static void printPattern(int n) {
	for (int i=1;i<=n;i++) {
		for(int j=1;j<=i;j++) {
			System.out.print(i+" ");
		}
		System.out.println();
	}
	
}
}
